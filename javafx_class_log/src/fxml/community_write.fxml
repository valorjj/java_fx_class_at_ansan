<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
	prefWidth="800.0" style="-fx-background-color: ffffff;"
	xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.CommunityWriteController">
	<children>
		<TextField fx:id="txttitle" layoutX="100.0" layoutY="68.0"
			prefHeight="37.0" prefWidth="587.0" promptText="제목을 입력하세요 "
			AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0"
			AnchorPane.topAnchor="68.0" />
		<TextArea fx:id="txtcontent" layoutX="100.0" layoutY="123.0"
			prefHeight="377.0" prefWidth="600.0" promptText="내용을 입력하세요"
			AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0"
			AnchorPane.topAnchor="123.0" />
		<Button fx:id="btn_write" layoutX="100.0" layoutY="518.0"
			mnemonicParsing="false" onAction="#write" prefHeight="51.0"
			prefWidth="240.0" text="REGISTER" AnchorPane.leftAnchor="100.0">
			<effect>
				<InnerShadow />
			</effect>
		</Button>
		<Button fx:id="btn_cancel" layoutX="460.0" layoutY="518.0"
			mnemonicParsing="false" onAction="#cancel" prefHeight="51.0"
			prefWidth="240.0" text="CANCEL" AnchorPane.rightAnchor="100.0">
			<effect>
				<InnerShadow />
			</effect>
		</Button>
		<Label layoutX="101.0" layoutY="18.0"
			style="-fx-font-family: Fira Code;" text="My Community "
			textFill="#7863b2">
			<font>
				<Font size="25.0" />
			</font>
		</Label>
		<Line endX="100.0" layoutX="201.0" layoutY="55.0" startX="-100.0" />
	</children>
</AnchorPane>
